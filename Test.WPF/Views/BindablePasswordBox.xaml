<UserControl x:Class="Test.WPF.Views.BindablePasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Test.WPF.Views"
             mc:Ignorable="d"
             Background="White"
             Width="110"
             Height="25"
             d:DesignHeight="25" d:DesignWidth="110">
    <Grid>
        <PasswordBox x:Name="PasswordBox" PasswordChanged="PasswordBox_PasswordChanged"/>
    </Grid>
</UserControl>
